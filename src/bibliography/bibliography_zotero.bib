  shorttitle = {Deep Brain Stimulation},
  author = {Lozano, Andres M. and Lipsman, Nir and Bergman, Hagai and Brown, Peter and Chabardes, Stephan and Chang, Jin Woo and Matthews, Keith and McIntyre, Cameron C. and Schlaepfer, Thomas E. and Schulder, Michael and Temel, Yasin and Volkmann, Jens and Krauss, Joachim K.},
  year = {2019},
