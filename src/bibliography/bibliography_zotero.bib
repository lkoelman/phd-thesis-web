@article{ding_thalamic_2010,
  title = {Thalamic {{Gating}} of {{Corticostriatal Signaling}} by {{Cholinergic Interneurons}}},
  author = {Ding, Jun B. and Guzman, Jaime N. and Peterson, Jayms D. and Goldberg, Joshua A. and Surmeier, D. James},
  year = {2010},
