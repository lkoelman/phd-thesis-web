  title = {Subthalamic Suppression Defines Therapeutic Threshold of Deep Brain Stimulation in {{Parkinson}}'s Disease},
  author = {Milosevic, Luka and Kalia, Suneil K and Hodaie, Mojgan and Lozano, Andres and Popovic, Milos R and Hutchison, William},
  year = {2019},
  month = oct,
  volume = {90},
  pages = {1105--1108},
  issn = {0022-3050, 1468-330X},
  doi = {10/gf8s9s},
  abstract = {Introduction\hspace{0.6em} Subthalamic deep brain stimulation (DBS) is beneficial when delivered at a high frequency. However, the effects of current amplitude and pulse width on subthalamic neuronal activity during highfrequency stimulation have not been investigated.
Methods\hspace{0.6em} In 20 patients with Parkinson's disease each undergoing subthalamic DBS, we recorded single-unit subthalamic activity using one microelectrode, while a separate microelectrode was used to deliver 5\textendash 10\,s trains of stimulation at 100\,Hz using varying current amplitudes and pulse widths (44 neurons investigated).
Results\hspace{0.6em} Analysis of variance tests confirmed significant (p{$<$}0.001) main effects of both current amplitude and pulse width on subthalamic neuronal firing during stimulation and on poststimulus inhibitory silent periods. Prolonged silent periods were often followed by postinhibitory rebound burst excitations. Additionally, a significant (p{$<$}0.0001) correlation was found between neuronal firing and total electrical energy delivered (TEED). With TEED values{$\leq$}31.2 \textmu J/s (associated with DBS parameters of {$\leq$}2.0\,mA, 130\,Hz stimulation frequency and 60 \textmu s pulse width, assuming 1\,k{$\Omega$} impedance), neuronal firing was sustained at a rate of 32.4\%{$\pm$}3.3\% (mean{$\pm$}SE), while with values{$>$}31.2 \textmu J/s, neurons fired at only 4.3\%{$\pm$}1.2\%.
Conclusions\hspace{0.6em} Neuronal suppression is likely an important mechanism of action of therapeutically beneficial subthalamic DBS, which may underlie clinically relevant behavioural changes.},
  file = {/home/luye/cloudstore_m/pubs_annotated/BG/DBS_experiments/milosevic2019_StnSuppressionDefinesTherapeuticThresholdDbs.pdf},
  journal = {Journal of Neurology, Neurosurgery \& Psychiatry},
  language = {en},
  number = {10}
}

